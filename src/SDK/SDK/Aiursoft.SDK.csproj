<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>7.0.28</Version>
    <TargetFramework>net7.0</TargetFramework>
    <AssemblyName>Aiursoft.SDK</AssemblyName>
    <RootNamespace>Aiursoft.SDK</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Company>Aiursoft</Company>
    <Product>SDK</Product>
    <Description>The base class, tools and extends for Aiursoft web apps.</Description>
    <PackageId>Aiursoft.SDK</PackageId>
    <PackageTags>nuget package dotnet csproj dependencies</PackageTags>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Aiursoft.WebTools" Version="7.0.9" />
    <PackageReference Include="Aiursoft.DbTools.SqlServer" Version="7.0.10" />
    <PackageReference Include="Aiursoft.DbTools.InMemory" Version="7.0.8" />
    <PackageReference Include="Aiursoft.AiurProtocol.Server" Version="7.0.12" />
    <PackageReference Include="Aiursoft.DocGenerator" Version="7.0.1" />
    <None Include="../../../Readme.md" Pack="true" PackagePath="." />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Infrastructure\Observer.SDK\Aiursoft.Observer.SDK.csproj" />
  </ItemGroup>
</Project>