@{
    ViewData["Title"] = "DocGenerator";
    ViewData["Des"] = "A basic API document generator for ASP.NET Core applications. Open source, offline and free.";
}

<div class="jumbotron">
    <div class="container">
        <h3>Aiursoft DocGenerator</h3>
        <p>@ViewBag.Des</p>
        <div class="btn-group" role="group">
            <a class="btn btn-primary btn-lg" href="https://gitlab.aiursoft.cn/aiursoft/infrastructures/-/tree/master/src/WebExtends/DocGenerator" target="_blank" role="button">
                <i class="fab fa-gitlab"></i>
                View it on GitLab
            </a>
        </div>
    </div>
</div>

<div class="container">
    <div class="row mt-3">
        <div class="col-md-12">
            <vc:git-hub-renderer org="AiursoftWeb" repo="Infrastructures" path="src/WebExtends/DocGenerator/Readme.md"></vc:git-hub-renderer>
        </div>
    </div>
</div>