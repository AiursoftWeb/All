<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <Version>6.0.7</Version>
	  <TargetFramework>net6.0</TargetFramework>
    <Company>Aiursoft</Company>
    <Product>AiursoftWeb</Product>
    <Authors>Anduin, One</Authors>
    <Description>A basic API document generator for ASP.NET Core applications</Description>
    <PackageTags>doc-generator api</PackageTags>
    <PackageProjectUrl>https://github.com/AiursoftWeb/DocGenerator</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/AiursoftWeb/DocGenerator.git</RepositoryUrl>
