<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <Version>8.0.0</Version>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>Aiursoft.Warpgate.SDK</AssemblyName>
    <RootNamespace>Aiursoft.Warpgate.SDK</RootNamespace>
    <IsTestProject>false</IsTestProject>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IsPackable>true</IsPackable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Company>Aiursoft</Company>
    <Product>SDK</Product>
    <Description>Nuget package of 'SDK' provided by Aiursoft</Description>
    <PackageId>Aiursoft.Warpgate.SDK</PackageId>
    <PackageTags>nuget package dotnet csproj dependencies</PackageTags>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Abstractions" Version="8.0.2" />
    <None Include="../../../Readme.md" Pack="true" PackagePath="." />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Directory.SDK\Aiursoft.Directory.SDK.csproj" />
  </ItemGroup>
</Project>