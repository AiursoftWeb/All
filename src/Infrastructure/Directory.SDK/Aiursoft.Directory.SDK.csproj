<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <Version>7.0.1</Version>
        <TargetFramework>net7.0</TargetFramework>
        <AssemblyName>Aiursoft.Directory.SDK</AssemblyName>
        <RootNamespace>Aiursoft.Directory.SDK</RootNamespace>
        <IsTestProject>false</IsTestProject>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <IsPackable>true</IsPackable>
        <ImplicitUsings>enable</ImplicitUsings>
        <Company>Aiursoft</Company>
        <Product>SDK</Product>
        <Description>Nuget package of 'SDK' provided by Aiursoft</Description>
        <PackageId>Aiursoft.Directory.SDK</PackageId>
        <PackageTags>nuget package dotnet csproj dependencies</PackageTags>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components" Version="7.0.10" />
        <PackageReference Include="Aiursoft.AiurProtocol" Version="7.0.0" />
        <PackageReference Include="Aiursoft.Scanner" Version="7.0.0" />
        <PackageReference Include="Aiursoft.DocGenerator.Abstractions" Version="6.0.25" />
    </ItemGroup>
</Project>